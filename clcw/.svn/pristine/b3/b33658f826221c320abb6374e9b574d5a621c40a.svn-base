    update wt_company
    set COMPANY_NAME = #{companyName,jdbcType=VARCHAR},
      OPER_ID = #{operId,jdbcType=VARCHAR},
      OPER_TIME = now()
    where COMPANY_ID = #{companyId,jdbcType=VARCHAR}
  </update>

  <sql id="Where_Condition" >
