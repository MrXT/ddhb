package com.manzz.clcw.common.enums.constant;

import com.manzz.clcw.common.enums.ConstantEnum;


/** 班次状态枚举类   */
public class FrequencyStatusEnum extends ConstantEnum{
	private static final long serialVersionUID = 4412447708654709573L;
	/** -1关闭 */
    public static final FrequencyStatusEnum CLOSE    = new FrequencyStatusEnum("关闭", "-1");
    /** 1开启 */
    public static final FrequencyStatusEnum OPEN  = new FrequencyStatusEnum("开启", "1");
    
    public FrequencyStatusEnum(String label, String value) {
        super(label, value);
    }
    
    public static void main(String[] args) {
        System.out.println(FrequencyStatusEnum.CLOSE);
        System.out.println(FrequencyStatusEnum.CLOSE.getValue());
        System.out.println(FrequencyStatusEnum.CLOSE.getIntValue());
    }
}